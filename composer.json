{
    "name": "mechanic/composer-cqrs",
    "type": "composer-plugin",
    "description": "Composer plugin for making CQRS class stubs",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "simpson",
            "email": "simpsonwork@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^2.0",
        "symfony/filesystem": "^5.2",
        "symfony/string": "^5.2"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mechanic\\ComposerCqrs\\": "src"
        }
    },
    "extra": {
        "class": "Mechanic\\ComposerCqrs\\Plugin"
    }
}
